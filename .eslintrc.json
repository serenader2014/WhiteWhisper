{
  "parser": "babel-eslint",
  "extends": ["airbnb"],
  "plugins": [
    "react"
  ],
  "rules": {
     "indent": ["error", 4, {"SwitchCase": 1}],
     "no-multi-spaces": ["error", { "exceptions": { "VariableDeclarator": true, "ImportDeclaration": true } }],
     "key-spacing": ["error", { "align": "colon" }],
     "react/jsx-indent": [2, 4],
     "react/jsx-indent-props": [2, 4]
  },
  "globals": {
    "config": false
  }
}
